{
  "name": "paste-string-filter",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Filter pasting string with regexp settings.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
    "matches": [
      "https://*/*"
    ],
    "js": ["dist/index.bundle.js"]
    }
  ]
}
